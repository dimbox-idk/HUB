task.spawn(function()
    print("InjectedðŸ˜Ž")
    getgenv().getsenv = newcclosure(function(script_instance)
        for i, v in pairs(getreg()) do
            if type(v) == "function" then
                if getfenv(v).script == script_instance then
                    return getfenv(v)
                end
           end
        end
    end)
end)
